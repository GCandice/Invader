<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="fr.iutlens.mmi.linvasion">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <service
            android:name="fr.iutlens.mmi.linvasion.MusicService"
            android:enabled="true" />

        <activity android:name="fr.iutlens.mmi.linvasion.MainActivity" />
        <activity android:name="fr.iutlens.mmi.linvasion.Main2Activity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="fr.iutlens.mmi.linvasion.result" />
        <activity android:name="fr.iutlens.mmi.linvasion.VictoryActivity"></activity>
    </application>

</manifest>